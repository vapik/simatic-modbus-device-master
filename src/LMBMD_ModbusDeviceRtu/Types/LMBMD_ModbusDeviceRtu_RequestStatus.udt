TYPE "LMBMD_ModbusDeviceRtu_RequestStatus"
VERSION : 0.1
//Modbus request status
   STRUCT
      req : Bool;   // Send request
      done : Bool;   // Request is done
      busy : Bool;   // Request is in progress
      error : Bool;   // Error
      status : Word;   // Error code 
      errorCounter : UInt;   // Counter of failed attempts
   END_STRUCT;

END_TYPE

